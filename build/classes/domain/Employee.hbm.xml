<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="domain.Employee" table="employee" catalog="productsormdb">
		<id name="employeeid" type="java.lang.Integer">
			<column name="employeeid" />
			<generator class="identity" />
		</id>
		<property name="employeelastname" type="string">
			<column name="employeelastname" length="80" />
		</property>
		<property name="mgrlastname" type="string">
			<column name="mgrlastname" length="80" />
		</property>
	</class>
	
</hibernate-mapping>
