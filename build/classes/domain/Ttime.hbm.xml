<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="domain.Ttime" table="ttime" catalog="productsormdb">
		<id name="timeid" type="java.lang.Integer">
			<column name="timeid" />
			<generator class="identity" />
		</id>
		<property name="tdate" type="timestamp">
			<column name="tdate" length="19" not-null="true" />
		</property>
	</class>
	
</hibernate-mapping>
